COLUMN TABLE "native::Scooter" (
"ID" NVARCHAR(32) COMMENT 'Scooter ID',
"MODELID" INTEGER COMMENT 'Model ID',
"CHARGELEVEL" INTEGER COMMENT 'Level of charge',
"LOCATIONLAT" DOUBLE COMMENT 'Location lat', 
"LOCATIONLNG" DOUBLE COMMENT 'Location lng',
"DESCRIPTIONID" NVARCHAR(32) COMMENT 'Description ID',
"STATUS" NVARCHAR(4) COMMENT 'Scooter status',
PRIMARY KEY("ID")
)