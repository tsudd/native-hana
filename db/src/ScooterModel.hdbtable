COLUMN TABLE "native::ScooterModel" (
  "ID" NVARCHAR(16) NOT NULL COMMENT 'Model ID',
  "NAME" NVARCHAR(100) COMMENT 'Name',
  "MANUFACTUREDATE" DATE COMMENT 'Date of manufacture',
  "DESCRIPTIONID" NVARCHAR(32) COMMENT 'Description ID',
  "LANGUAGE" NVARCHAR(1) DEFAULT 'E' COMMENT 'LANGUAGE',
  PRIMARY KEY ("ID")
)